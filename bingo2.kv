#:kivy 1.0.9
#:import Label kivy.uix.label.Label
#:import Button kivy.uix.button.Button

<BingoButton@Button>
    background_normal:''
    background_color:.4,.4,.4,1
    text_size: self.size
    font_size: self.height * 0.4
    valign:'middle'
    halign:'center'

<PartyButton@Button>
    background_normal:''
    background_color:.8,.4,.4,1
    valign:'middle'
    halign:'center'
    padding:10,10
    size_hint:.8,.8
<Bingo2Main>:
    rgbFrame: 0,1,1,0
    #on_touch_down: root.callback(args[1])
    id:main
    BoxLayout:
        canvas.before:
            Color:
                rgb: root.rgbFrame
            Rectangle:
                size: self.size
        pos:root.pos
        size:root.size
        GridLayout:
            rows:3
            pos:root.pos
            size:root.size
            GridLayout:
                size_hint: 1,.1
                cols: 1
                rows: 1
                Label:
                    id: ongoingParty
                    text: "Pas de partie en cours"
                    halign: 'center'
                    valign: 'middle'
                    text_size: self.width, None
                    font_size: self.height * 0.6
                    color: 0,0,0,1
            GridLayout:
                canvas.before:
                    Color:
                        rgb: root.rgbFrame
                    Rectangle:
                        size: self.size
                spacing:5
                size_hint:1,.6
                padding:10
                cols:1
                rows:1
                Label:
                    id: mainFrame
                    text:""
                    color:0,0,0,1
                    size: self.size
                    pos: self.pos
                    halign: 'center'
                    valign: 'middle'
                    text_size: self.width, None
                    font_size: self.height * 0.8
            GridLayout:
                canvas.before:
                    Color:
                        rgb: root.rgbFrame
                    Rectangle:
                        size: self.size
                size_hint: (1,0.3)
                cols:30
                rows:3
                spacing:5
